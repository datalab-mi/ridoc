{
  "name": "browser",
  "description": "browser",
  "scripts": {
    "dev": "sapper dev",
    "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
    "build": "npm run build:tailwindcss && sapper build --legacy",
    "export": "npm run build:tailwindcss && sapper export --legacy && mv ViewerJS/* ./__sapper__/export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@gouvfr/dsfr": "1.10.2",
    "compression": "1.7.4",
    "marked": "0.7.0",
    "polka": "1.0.0-next.24",
    "rollup": "2.79.1",
    "simple-svelte-autocomplete": "1.2.4",
    "svelte-paginate": "0.0.1",
    "svelte-tags-input": "2.9.2"
  },
  "devDependencies": {
    "npm-run-all": "4.1.5",
    "sapper": "0.27.16",
    "svelte": "3.59.2",
    "@babel/core": "7.23.5",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.23.4",
    "@babel/preset-env": "7.23.5",
    "@babel/runtime": "7.0.0",
    "@rollup/plugin-babel": "5.3.1",
    "@rollup/plugin-commonjs": "12.0.0",
    "@rollup/plugin-node-resolve": "8.4.0",
    "@rollup/plugin-replace": "2.4.2",
    "rollup": "2.3.4",
    "rollup-plugin-svelte": "5.2.3",
    "rollup-plugin-terser": "5.3.1",
    "postcss": "8.0.3",
    "postcss-cli": "7.1.2",
    "svelte-preprocess": "4.6.9",
    "tailwindcss": "1.8.10",
    "svelte-paginate": "0.0.1"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  }
}