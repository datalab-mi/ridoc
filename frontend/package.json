{
  "name": "browser",
  "description": "browser",
  "scripts": {
    "dev": "sapper dev",
    "dev:tailwindcss": "postcss static/tailwind.css -o static/main.css -w",
    "build:tailwindcss": "NODE_ENV=production postcss static/tailwind.css -o static/main.css",
    "build": "npm run build:tailwindcss && sapper build --legacy",
    "export": "npm run build:tailwindcss && sapper export --legacy",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.0.0",
    "compression": "^1.7.1",
    "marked": "^0.7.0",
    "polka": "next",
    "simple-svelte-autocomplete": "1.2.4",
    "sirv": "^0.4.0",
    "svelte-tags-input": "^2.6.5"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "sapper": "^0.27.0",
    "svelte": "^3.32.3",
    "@babel/core": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "@rollup/plugin-babel": "^5.0.0",
    "@rollup/plugin-commonjs": "^12.0.0",
    "@rollup/plugin-node-resolve": "^8.0.0",
    "@rollup/plugin-replace": "^2.2.0",
    "rollup": "^2.3.4",
    "rollup-plugin-svelte": "^5.0.1",
    "rollup-plugin-terser": "^5.3.0",
    "postcss": "8.0.3",
    "postcss-cli": "7.1.2",
    "svelte-preprocess": "4.6.x",
    "tailwindcss": "1.8.10"
  },
  "peerDependencies": {
    "postcss": "^8.0.0"
  }
}
